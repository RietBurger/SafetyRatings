@model IEnumerable<SafetyRatings.Models.JokeViewModel>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>10 Jokes</h2>


<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.type)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.setup)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.punchline)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.type)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.setup)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.punchline)
        </td>
    </tr>
}

</table>
